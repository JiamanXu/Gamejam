using System.Collections;
using System.Collections.Generic;
using UnityEngine;
using UnityEngine.UI;

public class UIManager : MonoBehaviour
{
    public GameObject Canvasbegin;
    public GameObject Canvaspreplot;
    public Button bottle;

    void Start()
    {
        // ��ʼ��Canvas״̬
        Canvasbegin.SetActive(true);
        Canvaspreplot.SetActive(false);

        // ��ť�����ת�¼�
        if (bottle != null)
        {
            bottle.onClick.AddListener(ShowCanvaspreplot);
        }
    }

    void ShowCanvaspreplot()
    {
        Canvasbegin.SetActive(false);
        Canvaspreplot.SetActive(true);
    }
}
